{
  "extends": "./.quasar/tsconfig.json",
  "compilerOptions": {
    "jsx": "preserve",
    "importHelpers": true,
    "noImplicitAny": true,
    "moduleResolution": "node",
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "types": ["quasar", "node"],
    "skipLibCheck": true,
    "jsxImportSource": "vue"
  },
  "include": ["src/**/*.ts", "src/**/*.d.ts", "src/**/*.tsx", "src/**/*.vue"]
}
